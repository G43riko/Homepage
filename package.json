        "@angular/router": "^8.2.1",
        "@g43/common": "0.0.6",
        "@types/angularfire": "^0.8.33",
        "@types/firebase": "^3.2.1",
        "core-js": "^3.2.0",
        "firebase": "^6.3.5",
