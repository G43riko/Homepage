{
    "name": "homepage-fe",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "serve": "ng serve --configuration=localhost",
        "serve:mock": "ng serve --configuration=mock",
        "build:prod": "ng build --configuration=production",
        "build:stats": "ng build --configuration=production --stats-json",
        "build:github": "ng build --configuration=production --outputPath=docs",
        "analyze": "webpack-bundle-analyzer dist/stats.json",
        "start": "node server.js",
        "precache": "sw-precache --verbose --config=sw-precache-config.js",
        "postinstall": "npm run build:prod && npm run precache",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/G43riko/Homepage-FE.git"
    },
    "author": {
        "email": "gcsollei@hotmail.com",
        "name": "Gabriel Csollei"
    },
    "keywords": [
        "Homepage",
        "Angular",
        "G43RIKO"
    ],
    "bugs": {
        "email": "gcsollei@hotmail.com",
        "url": "https://github.com/G43riko/Homepage-FE/issues"
    },
    "homepage": "http://csollei.herokuapp.com",
    "private": true,
